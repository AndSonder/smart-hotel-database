.total_box{
  display: flex;
  width: 100%;
  height: 1274rpx;
  flex-direction: column;
  justify-content: start;
  align-items: center;
}

.first_box{
  display: flex;
  width: 100%;
  height: 500rpx;
  flex-direction: column-reverse;
  justify-content: start;
  align-items: center;
}

.airImg_module{
  display: flex;
  width: 500rpx;
  height: 400rpx;
  justify-content: center;
  align-items: center;
}

.airImgContent_module{
  width: 100%;
  height: 100%;
}

.airImgContent{
  width: 100%;
  height: 100%;
}

.second_box{
  display: flex;
  width: 100%;
  height: 426rpx;
  justify-content: center;
  align-items: center;
}

.airStatus_module{
  display: flex;
  width: 100%;
  height: 200rpx;
  justify-content: space-around;
  align-items: center;
}

.airStatusContent_module{
  display: flex;
  width: 180rpx;
  height: 180rpx;
  border-radius: 90rpx;
  background-color: #f7f7f7;
  justify-content: center;
  align-items: center;

}

.airStatusContentShow_module{
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
}

.airStatusContentShow{
  width: 100%;
  height: 100%;
  overflow: hidden;

}

.third_box{
  display: flex;
  width: 100%;
  height: 350rpx;
  flex-direction: column;
  justify-content: start;
  align-items: center;
}

.control_module{
  display: flex;
  width: 200rpx;
  height: 206rpx;
  justify-content: center;
  align-items: center;
}

.controlContent_moduleP{
  width: auto;
  height: auto;
}

.switch_module{
  display: flex;
  width: 500rpx;
  height: auto;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  top: 1120rpx;
}
