/* pages/tpl/own-room/own-room.wxss */
.rooms-group{
  display: flex;
  width: 700rpx;
  height: 400rpx;
  flex-direction: column;
  background-color: #FBFBFB;
  font-family: "KaiTi";
  margin-top: 1em;
  margin-left: 25rpx;
  border-radius: 50rpx;
  box-shadow: 0rpx 10rpx 10rpx 5rpx rgba(26, 2, 2, 0.1);
}
.on-group{
  display: flex;
  width: 100%;
  height: 150rpx;
  text-align: center;
  padding-top: 20rpx;
  align-items: center;
  justify-content: center;
}


.center-group{
  display: flex;
  justify-content: space-evenly;
  width: 100%;
  height: 250rpx;
  
}
.pho-group{
  width:120rpx;
  height:120rpx;
}
.pho-group .pho-logo{
  width: 100%;
  height: 100%;
}
.down-group{
  display: flex;
  justify-content: space-evenly;
  width: 100%;
  height: 130rpx;
  margin-left: 20rpx;
}



